    "jasmine-core": "^5.13.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "netlify-cli": "^23.13.0",
    "ng-mocks": "^14.15.0",
    "stylelint": "^16.26.1",
