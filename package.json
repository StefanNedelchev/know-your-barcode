    "@angular/router": "^18.0.6",
    "@angular/service-worker": "^18.0.6",
    "@netlify/functions": "^2.8.1",
    "axios": "^1.7.2",
    "barcode-detector-api-polyfill": "^1.0.14",
    "bootstrap": "^5.3.3",
