    "@angular/forms": "^19.1.5",
    "@angular/platform-browser": "^19.1.5",
    "@angular/platform-browser-dynamic": "^19.1.5",
    "@angular/router": "^19.1.5",
    "@angular/service-worker": "^19.1.5",
    "@netlify/functions": "^2.8.2",
    "axios": "^1.7.9",
    "barcode-detector-api-polyfill": "^1.0.16",
    "bootstrap": "^5.3.3",
    "cheerio": "^1.0.0",
