    "@angular/platform-browser": "^21.0.6",
    "@angular/platform-browser-dynamic": "^21.0.6",
    "@angular/router": "^21.0.6",
    "@angular/service-worker": "^21.0.6",
    "@netlify/functions": "^5.1.2",
    "axios": "^1.13.2",
    "barcode-detector-api-polyfill": "^1.0.20",
    "bootstrap": "^5.3.8",
    "cheerio": "^1.1.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.1"
  },
