    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "netlify-cli": "^17.38.0",
    "ng-mocks": "^14.13.2",
    "stylelint": "^16.11.0",
